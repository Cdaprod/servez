    "express": "^4.15.0",
    "serve-index": "^1.8.0",
    "server-destroy": "^1.0.1"
  },
  "devDependencies": {
    "cross-env": "^3.1.4",
    "electron": "^1.6.1",
    "electron-builder": "^14.5.3"
