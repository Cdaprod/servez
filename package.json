{
  "name": "Servez",
  "version": "1.0.0",
  "description": "A Simple Webserver for Localdev with GUI",
  "main": "main.js",
  "build": {
    "appId": "com.greggman.servez",
    "asar": true,
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  },
  "scripts": {
